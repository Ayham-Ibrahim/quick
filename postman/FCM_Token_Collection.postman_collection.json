			"value": "http://localhost:8000/api",
			"type": "string"
		},
		{
			"key": "userToken",
			"value": "",
			"type": "string"
		},
		{
			"key": "driverToken",
			"value": "",
			"type": "string"
		},
		{
			"key": "providerToken",
			"value": "",
			"type": "string"
		},
		{
			"key": "storeToken",
			"value": "",
			"type": "string"
		}
	],
