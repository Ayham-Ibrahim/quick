					"// Auto-save token from login response",
					"if (pm.response.code === 200) {",
					"    var jsonData = pm.response.json();",
					"    if (jsonData.data && jsonData.data.token) {",
					"        // Determine which token to save based on user type",
					"        if (jsonData.data.user && jsonData.data.user.type) {",
					"            switch(jsonData.data.user.type) {",
					"                case 'store':",
					"                    pm.collectionVariables.set('storeToken', jsonData.data.token);",
					"                    break;",
					"                case 'admin':",
					"                    pm.collectionVariables.set('adminToken', jsonData.data.token);",
					"                    break;",
					"                default:",
					"                    pm.collectionVariables.set('userToken', jsonData.data.token);",
					"            }",
					"        } else {",
					"            pm.collectionVariables.set('userToken', jsonData.data.token);",
					"        }",
					"    }",
					"}"
				]
			}
